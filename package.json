{
  "name": "x-datetimepicker-vue3",
  "version": "1.0.5",
  "keywords": [
    "datetime picker",
    "vue3 datetime picker"
  ],
  "description": "A datetime picker with vue3",
  "private": false,
  "license": "MIT",
  "main": "./dist/x-datetime-picker.umd.js",
  "module": "./dist/x-datetime-picker.es.js",
  "exports": {
    ".": {
      "import": "./dist/x-datetime-picker.es.js",
      "require": "./dist/x-datetime-picker.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lib": "vue-cli-service build --target lib --name xdatetimepicker --dest lib packages/index.js"
  },
  "peerDependencies": {
    "dayjs": "^1.10.8",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "vite": "^2.8.4"
  }
}
